
<br><br>
<div class="container">
    <h1>Log In</h1>
    <br><br>
    <%= form_with url: '/login', local: true do | f | %>
        <%= f.label :email%>
        <%= f.text_field :email %>
        <br><br>
        <%= f.label :password %>
        <%= f.password_field :password %>
        <br><br>
        <%= f.submit "Log In" %>
    <% end %>
</div>