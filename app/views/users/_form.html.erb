<%= form_with model: @user, local: true do | f | %>
    <%= f.label :name %>
    <%= f.text_field :name, placeholder: @user.name %>
    <br><br>
    <%= f.label :username %>
    <%= f.text_field :username, placeholder: @user.username %>
    <br><br>
    <%= f.label :email %>
    <%= f.email_field :email, placeholder: @user.email %>
    <br><br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br><br>
    <%= f.submit "Update Changes" %>
<% end %>